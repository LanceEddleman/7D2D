	<set xpath="//item[@name='ammoArrowStone']/property[@name='DropMeshfile']/@value">@:Other/Items/Weapons/Ranged/Bows/Arrows/arrow_flamingPrefab.prefab</set>

	<set xpath="//item[@name='ammoArrowFlaming']/property[@name='IsSticky']/@value">true</set>
	<append xpath="//item[@name='ammoArrowFlaming']">
		<property name="StickyMaterial" value="@:Other/Items/Weapons/Melee/Spear/Materials/spear_sticky.mat" CustomIconTint="ff4d4d" TintColor="255, 77, 77" />
