-->
	<!-- Pulled from Stasis' AugerNoKickMod - no kicks Updated for A19.2 -->
	<set xpath="//item[@name='meleeToolPickT3Auger']/effect_group/passive_effect[@name='KickDegreesVerticalMin']/@value">0.0</set>
	<set xpath="//item[@name='meleeToolPickT3Auger']/effect_group/passive_effect[@name='KickDegreesVerticalMax']/@value">0.0</set>
	<set xpath="//item[@name='meleeToolPickT3Auger']/effect_group/passive_effect[@name='KickDegreesHorizontalMin']/@value">0.0</set>
	<set xpath="//item[@name='meleeToolPickT3Auger']/effect_group/passive_effect[@name='KickDegreesHorizontalMax']/@value">0.0</set>

	<!-- Sticky arrow and bolt Claymore -->
	<append xpath="/items/item[@name='ammoArrowFlaming']">
		<property name="IsSticky" value="true"/>
	</append>
	<append xpath="/items/item[@name='ammoCrossbowBoltFlaming']">
		<property name="IsSticky" value="true"/>
	</append>

	<!-- sticky chance for flaming arrows and bolts. -->
	<set xpath="/items/item[@name='ammoArrowFlaming']/effect_group[@name='ammoArrowFlaming']/passive_effect[@name='ProjectileStickChance']/@value">50</set>
	<set xpath="/items/item[@name='ammoCrossbowBoltFlaming']/effect_group[@name='ammoCrossbowBoltFlaming']/passive_effect[@name='ProjectileStickChance']/@value">50</set>

	<!-- 100% Stick chance for all arrows and bolts Set to 75% -->
	<append xpath="/items/item[@name='ammoArrowStone']/effect_group[@name='ammoArrowStone']">
		<passive_effect name="ProjectileStickChance" operation="perc_set" value="50"/>
	</append>
	<append xpath="/items/item[@name='ammoArrowIron']/effect_group[@name='ammoArrowIron']">
		<passive_effect name="ProjectileStickChance" operation="perc_set" value="50"/>
	</append>
