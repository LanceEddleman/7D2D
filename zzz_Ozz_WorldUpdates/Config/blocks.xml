<Ozzland>

	<!-- Set Node colors for map -->
		<!--terrOreCoal original color "0,0,0" Black-->
		<set xpath="/blocks/block[@name='terrOreCoal']/property[@name='Map.Color']/@value">0,0,0</set>
		<!--terrOreLead original color "70,110,120" Changed to Blue -->
