<Ozzland>
	<!-- Birdnest egg chance -->
	<append xpath="/lootcontainers/lootgroup[@name='groupBirdNest01']">
		<item name="foodEgg" count="0,3" loot_prob_template="high"/>
	</append>
	
	<remove xpath="/lootcontainers/lootgroup[@name='groupBirdNest02']/item[@name='foodEgg']"/>
	<append xpath="/lootcontainers/lootgroup[@name='groupBirdNest02']">
		<item name="foodEgg" count="0,3" loot_prob_template="high"/>
	</append>

	<!-- Birdnest size -->
	<set xpath="/lootcontainers/lootcontainer[@name='birdNest']/@size">6,1</set>

	<!-- Adds acid bottles to cupboards -->
	<append xpath="/lootcontainers/lootgroup[@name='groupCupboard01']">
		<item name="resourceAcid" count="1,2" loot_prob_template="low" />
	</append>

	<!-- Increases the number of lockpicks found in lootgroups -->
	<set xpath="/lootcontainers/lootgroup/item[@name='resourceLockPick']/@count">1,3</set>
</Ozzland>
